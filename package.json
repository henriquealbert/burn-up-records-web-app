{
  "name": "burn-up-records",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src",
    "next:lint": "next lint",
    "postinstall": "husky install",
    "update": "yarn upgrade-interactive --latest",
    "schema:download": "graphql-codegen --require dotenv/config --config ./src/graphql/schema.yml dotenv_config_path=.env.local",
    "graphql:codegen": "yarn schema:download && graphql-codegen --config ./src/graphql/codegen.yml && yarn eslint src/graphql --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.8",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11",
    "@hookform/resolvers": "^2.8.1",
    "aws-sdk": "^2.991.0",
    "axios": "^0.21.4",
    "date-fns": "^2.24.0",
    "framer-motion": "^4.1.17",
    "graphql": "^15.6.0",
    "js-cookie": "^3.0.1",
    "nanoid": "^3.1.25",
    "next": "^11.1.2",
    "next-auth": "^3.29.0",
    "polished": "^4.1.3",
    "react": "^17.0.2",
    "react-day-picker": "^7.4.10",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.15.4",
    "react-icons": "^4.2.0",
    "react-query": "^3.24.3",
    "react-select": "^4.3.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/schema-ast": "^2.2.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-operations": "2.1.4",
    "@graphql-codegen/typescript-react-query": "^3.0.1",
    "@graphql-eslint/eslint-plugin": "^2.2.0",
    "@next/eslint-plugin-next": "^11.1.2",
    "@types/js-cookie": "^2.2.7",
    "@types/node": "^16.9.4",
    "@types/react": "^17.0.22",
    "@types/react-dom": "^17.0.9",
    "@types/react-select": "^4.0.17",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  }
}
